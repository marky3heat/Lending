<div class="page-header-content">
    <div class="page-title">   
        @if (Session["UserRank"].ToString() == "1")
        {
            <h4><i class="icon-arrow-left52 position-left"></i> <span class="text-semibold">Administrator</span> - @ViewBag.Form</h4>
        }
        @if (Session["UserRank"].ToString() == "2")
        {
            <h4><i class="icon-arrow-left52 position-left"></i> <span class="text-semibold">Super Administrator</span> - @ViewBag.Form</h4>
        }
        @if (Session["UserRank"].ToString() == "3")
        {
            <h4><i class="icon-arrow-left52 position-left"></i> <span class="text-semibold">User</span> - @ViewBag.Form</h4>
        }
    </div>
</div>
<div class="breadcrumb-line">
    <ul class="breadcrumb">
        <li><a href="#"><i class="icon-home2 position-left"></i> @ViewBag.Controller</a></li>
        <li class="active">@ViewBag.Action</li>
    </ul>
</div>